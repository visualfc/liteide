<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Railscast">
  <!--
  Based on the railscast theme for emacs/vim
  Improvements are welcome
  Public Domain, Alexander RÃ¸dseth, 2012
  -->
  <style name="Text" foreground="#bbbbbb" background="#232323"/>
  <style name="Extra" foreground="#008B8B" background="#232323"/>
  <style name="Selection" background="#555577"/>
  <style name="CurrentLine" background="#000000"/>
  <style name="IndentLine" foreground="#008B8B"/>
  <style name="VisualWhitespace" foreground="#c0c0c0"/>
  <style name="Keyword" foreground="#cc7833"/>
  <style name="DataType" foreground="#white" /> 
  <style name="Decimal" foreground="#6d9cbe"/>
  <style name="BaseN" foreground="#6d9cbe"/>
  <style name="Float" foreground="#6d9cbe"/>
  <style name="Char" foreground="#55c2bb"/>
  <style name="String" foreground="#a5c261"/>
  <style name="Comment" foreground="#bc9458"/>
  <style name="Alert" foreground="#ff0000"/>
  <style name="Error" foreground="#ff0000"/>
  <style name="Function" foreground="#ffc66d"/>
  <style name="RegionMarker" foreground="#bbbbbb"/>
  <style name="Symbol" foreground="#bbbbbb"/>  
  <style name="BuiltinFunc" foreground="#ffc66d"/>
  <style name="Predeclared" foreground="#ffc66d"/>
  <style name="FuncDecl" foreground="#bbbbbb"/>
  <style name="Placeholder" foreground="#c261c2"/>
  <style name="ToDo" foreground="#ff5523" bold="true"/>
</style-scheme>
