  -->
  <style name="Alert" foreground="#FF0000"/>
  <style name="BaseN" foreground="#FFAA00"/>
  <style name="BuiltinFunc" foreground="#80FFFF"/>
  <style name="Char" foreground="#EAFF00"/>
  <style name="Comment" foreground="#00FF00"/>
  <style name="CurrentLine" background="#333333"/>
  <style name="MatchBrackets" background="#808080"/>
