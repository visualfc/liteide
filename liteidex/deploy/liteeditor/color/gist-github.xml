<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Gist Github">
  <!--
  Based on Gist Github
  Improvements are welcome
  Public Domain, Ardan Studios, 2014
  -->
  <style name="Text" foreground="#000000" background="#f8f8ff"/>
  <style name="Extra" foreground="#008B8B" background=""/>
  <style name="Selection" background="#ADD6FF"/>
  <style name="CurrentLine" background="#B4C8C8"/>
  <style name="IndentLine" foreground="#008B8B"/>
  <style name="VisualWhitespace" foreground="#f57900"/>
  <style name="Keyword" foreground="#000000" bold="true"/>
  <style name="DataType" foreground="#445588" bold="true" /> 
  <style name="Decimal" foreground="#099" bold="true"/>
  <style name="BaseN" foreground="#099" bold="true"/>
  <style name="Float" foreground="#099" bold="true"/>
  <style name="Char" foreground="#d14"/>
  <style name="String" foreground="#d14"/>
  <style name="Comment" foreground="#7a7a6c"/>
  <style name="Alert" foreground="#ff0000"/>
  <style name="Error" foreground="#ff0000"/>
  <style name="Function" foreground="#000000"/>
  <style name="RegionMarker" foreground="#000000"/>
  <style name="Symbol" foreground="#000000"/> 
  <style name="BuiltinFunc" foreground="#000000" bold="true"/>
  <style name="Predeclared" foreground="#000000" bold="true"/>
  <style name="FuncDecl" foreground="#000000"/>
  <style name="Placeholder" foreground="#d14"/>
  <style name="ToDo" foreground="#006000" background="#e0e0e0" bold="true"/>
</style-scheme>
